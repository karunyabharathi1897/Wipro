package PatternPrinting;

import java.util.Scanner;

/**
 *
 * @author Dell
 */
public class P2 {
       public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int k,m;
        for(int i=1;i<=n;i++)
        {
            k=i;
            m=n;
            for(int j=i;j<=n;j++)
            {
                System.out.print(k+" ");
                k=k+m--;
                
            }
            System.out.println("");
        }
       }
}
