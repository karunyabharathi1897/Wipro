package javaapplication1;

import java.util.Scanner;

/**
 *
 * @author skcet
 */
public class patt2 {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int size=4;
        int k=0,h=0;
        for(int j=1;j<=size;j++)
        {
            h=1;
            k=j;
            for(int i=j;i<=size;i++)
            {
                System.out.print(i);
            }
            while(k>1)
            {
                System.out.print(h++);
                k--;
            }
            System.out.println("");
            
        }
    }
}
