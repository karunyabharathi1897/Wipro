package prp;

import java.util.Scanner;

/**
 *
 * @author skcet
 */
public class Substring {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        int k=0;
        while(k<t)
        {
            String str=sc.next();
            int l=sc.nextInt();
            int r=sc.nextInt();
            for(int i=0;i<str.length();i++)
            {
                if(i>=l && i<=r)
                {
                    System.out.print(str.charAt(i));
                }
            }
            System.out.println("");
        }
    }
}
